        "#@title\n",
        "from __future__ import print_function\n",
        "from ipywidgets import interact, interactive, fixed, interact_manual\n",
        "import ipywidgets as widgets\n",
        "import math\n",
        "from IPython.display import clear_output\n",
        "\n",
        "# ALL DATA TAKEN FROM https://depts.washington.edu/eooptic/links/acidstrength.html.\n",
        "\n",
        "weak_acid_list = [None, \"oxalic (HO2C2O2H)\", \"sulfurous (H2SO3)\", \"hydrogen sulfate ion (HSO4 -)\", \"phosphoric (H3PO4)\", \"nitrous (HNO2)\", \"hydrofluoric (HF)\", \"methanoic (HCO2H)\",\"benzoic (C6H5COOH)\",\"hydrogen oxalate ion (HO2C2O 2-)\",\"acetic (CH3COOH)\",\"carbonic (CO3 2-)\", \"hydrosulfuric (H2S)\", \"dihydrogen phosphate ion (H2PO4 -)\", \"hydrogen sulfite ion (HS -)\",\"hypochlorous (S 2-)\", \"hydrocyanic (HCN)\", \"ammonium ion (NH4 +)\", \"boric (H3BO3)\", \"hydrogen carbonate ion (HCO3 -)\", \"hydrogen phosphate ion (HPO4 2-)\", \"dihydrogen borate ion (H2BO3 -)\",\"hydrogen sulfide ion (HS -)\", \"hydrogen borate ion (HBO3 2-)\"]\n",
        "\n",
        "conj_base_list = [None, \"hydrogen oxalate ion (HO2C2O2 -)\", \"hydrogen sulfite ion (HSO3 -)\", \"sulfate ion (SO4 2-)\", \"dihydrogen phosphate ion (H2PO4 -)\", \"nitrite ion (NO3 -)\", \"fluoride ion (F -)\", \"methanoate ion (HCO2 -)\", \"benzoate ion (C6H5COO -)\", \"oxalate ion (O2C2O2 2-)\", \"acetate ion (CH3COO)\", \"hydrogen carbonate ion (HCO3 -)\", \"hydrogen sulfide ion (HS-)\", \"hydrogen phosphate ion (HPO4 2-)\", \"sulfite ion (S 2-)\", \"hypochlorite ion (CLO -)\", \"cyanide ion (CN -)\", \"ammonia (NH3)\", \"Dihydrogen borate ion (H2BO3 -)\", \"carbonate ion (CO3 2-)\", \"phosphate ion (PO4 3-)\", \"hydrogen borate ion (HBO3 2-)\", \"sulfide ion (S 2-)\", \"borate ion (BO3 3-)\"]\n",
        "\n",
        "ka_list = [None, 5.4 * 10**-2, 1.3 * 10**-2, 1.0 * 10**-2,7.1 * 10**-3, 7.2 * 10**-4, 6.6 * 10**-4, 1.8 * 10**-4, 6.3 * 10**-5, 5.4 * 10**-5,1.8 * 10**-5, 4.4 * 10**-7, 1.1 * 10**-7, 6.3 * 10**-8, 6.2 * 10**-8, 2.9 * 10**-8, 6.2 * 10**-10, 5.8 * 10**-10, 5.8 * 10**-10, 4.7 * 10**-11, 4.2 * 10**-13, 1.8 * 10**-13, 1.3 * 10**-13, 1.6 * 10**-14]\n",
        "\n",
        "inputlist= ['weak acid','conj base','ka value','weak acid concentration', 'conjugate base concentration']\n",
        "\n",
        "pka_list = [None]\n",
        "\n",
        "substance_list = [None, None, None, None, None, ['Nitrous Acid (HNO2)', 'Sodium Nitrate (NaNO2)'], [\"Hydrofluoric Acid (HF)\", 'Sodium Fluoride (NaF)'], None, None, None, None, None, None, ['Monosodium Phosphate (NaH2PO4)','Disodium Phosphate (NaHPO4)'], None, None, None, None, ['Boric Acid (H3BO3)', 'Borax (Na2[B4O5(OH)4]8H2O)'], ['Baking Soda (NaHCO3)','Sodium Carbonate (Na2CO3)'],None, None, None, None]\n",
        "\n",
        "substance_mass_list = [None, None, None, None, None, [47.01, 84.99], [20.01, 41.99], None, None, None, None, None, None, [119.98,141.96], None, None, None, None, [61.83, 381.37], [84.01,105.99],None, None, None, None]\n",
        "\n",
        "\n",
        "#takes ka values and takes the -log or makes it pka value\n",
        "def kaToPka(ka): \n",
        "  return str(-(math.log10(ka)))\n",
        "\n",
        "for i in range (1, len(ka_list)):\n",
        "  pka_list.append(kaToPka(ka_list[i]))\n",
        "\n",
        "\n",
        "#input list of widget input values and outputs info for finding pka (specifically, \n",
        "#which pka calculation fields to use)\n",
        "def findKnownKaType(listtemp): \n",
        "  indexlist = []\n",
        "  for i in range(2,6):\n",
        "    if listtemp[i] != None and listtemp[i] != 0 and listtemp[i] != 7:\n",
        "      indexlist.append(i)\n",
        "  return indexlist\n",
        "\n",
        "#takes ph pka and weak acid concentration and returns conj base concentration\n",
        "def calcCB(ph, pka, wa): \n",
        "  conj = 10**(float(ph)-float(pka)) * float(wa)\n",
        "  return str(conj)\n",
        "\n",
        "#takes ph pka and conj base concetration and returns weak acid concentration\n",
        "def calcWA(ph, pka, cb):\n",
        "  weak = float(cb) / (10**(float(ph)-float(pka)))\n",
        "  return str(weak)\n",
        "\n",
        "#inputs ph wa conc and cb conc and returns pka\n",
        "def calcPka(ph, wa, cb):\n",
        "  pka = float(ph)-(math.log10(float(cb)/float(wa)))\n",
        "  return str(pka)\n",
        "\n",
        "#input list from findknownkatype and outputs pka value to use\n",
        "def findPka(listtemp): \n",
        "  if len(findKnownKaType(listtemp)) == 0:\n",
        "    return False\n",
        "  else:\n",
        "    if findKnownKaType(listtemp)[0] == 2:\n",
        "      return kaToPka(ka_list[weak_acid_list.index(listtemp[findKnownKaType(listtemp)[0]])])\n",
        "    elif findKnownKaType(listtemp)[0] == 3:\n",
        "      return kaToPka(ka_list[conj_base_list.index(listtemp[findKnownKaType(listtemp)[0]])])\n",
        "    elif findKnownKaType(listtemp)[0] == 4:\n",
        "      return kaToPka(listtemp[findKnownKaType(listtemp)[0]])\n",
        "    else:\n",
        "      return listtemp[5]\n",
        "\n",
        "#figures out unknown value\n",
        "def unknownFinder(templist):\n",
        "  if findPka(templist) == False:\n",
        "    return 'pka'\n",
        "  elif templist[6] == 0: #this checks if the waconc is input\n",
        "    return 'waconc'\n",
        "  elif templist[7] == 0: #checks if cbconc is input\n",
        "    return 'cbconc'\n",
        "  else:\n",
        "    return 'none'\n",
        "\n",
        "def closest(lst, K):  \n",
        "  return lst[min(range(1,len(lst)), key = lambda i: abs(float(lst[i])-float(K)))] \n",
        "\n",
        "#takes in pka_list and pka value and figures out the substance\n",
        "def substanceFinder(listtemp,pka):\n",
        "  index = listtemp.index(closest(listtemp, pka))\n",
        "  if index ==17:\n",
        "    index += 1\n",
        "  return weak_acid_list[index]\n",
        "\n",
        "#returns unknown value and a list of final values [ph, pka, waconc, cbconc]\n",
        "def unknownReturn(templist):\n",
        "  if unknownFinder(templist) == 'pka':\n",
        "    print(\"The weak acid in the buffer has a pKa of \" + calcPka(templist[0], templist[6],templist[7]) + \", and its identity is likely \" + substanceFinder(pka_list,calcPka(templist[0], templist[6],templist[7])))\n",
        "    return [templist[0],calcPka(templist[0], templist[6],templist[7]),templist[6], templist[7]]\n",
        "  elif unknownFinder(templist) == 'waconc':\n",
        "    print(\"The substance's weak acid concentration must be \" + calcWA(templist[0], findPka(templist), templist[7]) + ' moles per liter.')\n",
        "    return [templist[0],findPka(templist),calcWA(templist[0], findPka(templist), templist[7]), templist[7]]\n",
        "  elif unknownFinder(templist) == 'cbconc':\n",
        "    print(\"The subtance's conjugate base concentration must be \" + calcCB(templist[0],findPka(templist), templist[6]) + ' moles per liter.')\n",
        "    return [templist[0],findPka(templist), templist[6], calcCB(templist[0],findPka(templist), templist[6])]\n",
        "  else:\n",
        "    print(\"You filled in all the fields, so nothing has to be calculated. This program does not check your calculations, so be careful, as they may be wrong.\")\n",
        "    return [templist[0],findPka(templist), templist[6], templist[7]]\n",
        "\n",
        "#inputs ph, pka, and concentration values and outputs exact amounts per 1L solution\n",
        "def procedure(ph, pka, waconc, cbconc, vol):\n",
        "  indexpka = int(pka_list.index(closest(pka_list, pka)))\n",
        "  if indexpka ==17:\n",
        "    print('WARNING: Boric acid and Ammonium have virtually identical pka values, however, this program assumes that you are using Boric acid for procedure calculations.')\n",
        "    indexpka += 1\n",
        "  if substance_list[indexpka] == None:\n",
        "    print('Sorry, we do not have enough information about that acid-base pair.')\n",
        "    return False\n",
        "  else:\n",
        "    wamass = float(waconc) * (substance_mass_list[indexpka])[0] * (vol)\n",
        "    cbmass = float(cbconc) * (substance_mass_list[indexpka])[1] * (vol)\n",
        "    waname = substance_list[indexpka][0]\n",
        "    cbname = substance_list[indexpka][1]\n",
        "    print('Add ' + str(wamass) + 'g of ' + str(waname) + ' to ' + str(cbmass) + 'g of ' + str(cbname) + 'in ' + str(vol) + ' L of water (H2O).')\n",
        "    return True\n",
        "\n",
        "def capacityWarning(ph, pka):\n",
        "  if ph >= (float(pka)+1):\n",
        "    return('This buffer will not have an ideal capacity due to the large ratio of conjugate base to weak acid. This means that it will not be very effective at buffering bases.')\n",
        "  elif ph <= (float(pka)-1):\n",
        "    return ('This buffer will not have an ideal capacity due to the large ratio of weak acid to conjugate base. This means that it will not be very effective at buffering acids.')\n",
        "  else:\n",
        "    return(\"This buffer has an ideal capacity (meaning the ph = pka +- 1).\")\n",
        "\n",
        "#button and output widgets\n",
        "button = widgets.Button(description = \"Calculate\")\n",
        "output = widgets.Output()\n",
        "\n",
        "#input widgets\n",
        "phinput = widgets.BoundedFloatText(value = 8.2, min = 0, max = 14, description = 'pH')\n",
        "wainput = widgets.Dropdown(options = weak_acid_list, description = \"Weak Acids\")\n",
        "cbinput = widgets.Dropdown(options = conj_base_list, description = \"Conj Bases\")\n",
        "kainput = widgets.Dropdown(options = ka_list, description = \"Ka values\")\n",
        "waconc = widgets.BoundedFloatText(value = 1, min = 0, description = 'Weak Acid Concentration')\n",
        "cbconc = widgets.BoundedFloatText(value = 0, min = 0, description = 'Conjugate Base Concentration')\n",
        "pkainput = widgets.FloatText(value = 10.3, min = 0, max = 14, description = 'Pka value \\n')\n",
        "htmlinput = widgets.HTML(value=\"Select one of the following four or leave then blank to solve for pka and identity: \", description = \" \")\n",
        "volumeinput = widgets.BoundedFloatText(value = 0.3, min = 0, description = 'Volume of Buffer Solution Wanted (in litres)')\n",
        "\n",
        "#calculates unknown value when button is clicked\n",
        "def on_button_clicked(y):\n",
        "  with output:\n",
        "    clear_output()\n",
        "    finallist = unknownReturn(x.result)\n",
        "  #  print(finallist)\n",
        "    clear_output()\n",
        "    unknownReturn(x.result)\n",
        "    print(\"\\n\")\n",
        "    procedure(finallist[0],finallist[1],finallist[2],finallist[3], x.result[8])\n",
        "    print(\"\\n\")\n",
        "    print(capacityWarning(finallist[0],finallist[1]))\n",
        "    return x.result\n",
        "    #this is where we use all the calculations\n",
        "\n",
        "#gets input values for interactive widget\n",
        "def disp(ph, html, wa, cb, ka, pka, wac, cbc, vi):\n",
        "  return (ph, html, wa, cb, ka, pka, wac, cbc, vi)\n",
        "\n",
        "\n",
        "x = interactive(disp,ph = phinput, html = htmlinput,wa = wainput, cb = cbinput, \n",
        "                ka = kainput, pka = pkainput, wac = waconc, cbc =cbconc, vi = volumeinput )\n",
        "\n",
        "\n",
        "\n",
        "display(x)\n",
        "\n",
        "display(button,output)\n",
        "\n",
        "temp = button.on_click(on_button_clicked)\n",
        "\n",
        "if (temp != None):\n",
        "  print(temp)\n"
